//https://ac.nowcoder.com/acm/contest/889/D

#include<bits/stdc++.h>
#define F first
#define S second
using namespace std;
typedef long long ll;
typedef pair<ll,ll> pi;
int n,p,q;
set <ll> s;
map <ll,int> f;
ll a[36],sum;
int main()
{
    cin >> n;
    p=n/2;
    q=n-p;
    cout<<"n=="<<n<<endl;
    cout<<"p=="<<p<<endl;
    cout<<"q=="<<q<<endl;
    cin >> sum;
    for (int i=0;i<n;i++) cin >> a[i];
    for (int i=0;i<(1<<p);i++)
    {
        cout<<"i=="<<i<<endl;
        ll ret=0;
        for (int j=0;j<p;j++)
        {
            cout<<"j=="<<j<<endl;
          if (i&(1<<j))
          {
              cout<<i<<"&(1<<"<<j<<") is true"<<endl;
              cout<<"a["<<j<<"]=="<<a[j]<<endl;
              cout<<"ret=="<<ret<<endl;
              ret+=a[j];
              cout<<"ret=="<<ret<<endl;
          }
        }
        s.insert(ret);
        cout<<"s.insert("<<ret<<")"<<endl;
        f[ret]=i;
        cout<<"f["<<ret<<"]=="<<f[ret]<<endl;
    }
    cout<<endl;
    cout<<endl;
    for (int i=0;i<(1<<q);i++)
    {
        cout<<"i=="<<i<<endl;
        ll ret=0;
        for (int j=0;j<q;j++)
        {
            cout<<"j=="<<j<<endl;
            if (i&(1<<j))
            {
                cout<<i<<"&(1<<"<<j<<") is true"<<endl;
                cout<<"a["<<j<<"+"<<p<<"]=="<<a[j+p]<<endl;
                cout<<"ret=="<<ret<<endl;
                ret+=a[j+p];
                cout<<"ret=="<<ret<<endl;
            }
        }
        cout<<sum<<"-"<<ret<<"=="<<sum-ret<<endl;
        if (s.find(sum-ret)!=s.end())
        {
            cout<<"sum-ret found"<<endl;
            int x=f[sum-ret];
            cout<<"f["<<sum-ret<<"]=="<<f[sum-ret]<<endl;
            for (int j=0;j<p;j++)
            {
                cout<<"j=="<<j<<endl;
                cout<<x<<"&(1<<"<<j<<")=="<<endl;
                printf("%d",(bool)(x&(1<<j)));
                cout<<endl;
            }
            for (int j=0;j<q;j++)
            {
                cout<<"j=="<<j<<endl;
                cout<<i<<"&(1<<"<<j<<")=="<<endl;
                printf("%d",(bool)(i&(1<<j)));
                cout<<endl;
            }
            puts("");
            return 0;
        }
        cout<<endl;
        cout<<endl;
    }
    assert(0);
    return 0;
}
